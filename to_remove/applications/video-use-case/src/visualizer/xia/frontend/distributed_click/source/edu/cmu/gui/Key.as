package edu.cmu.gui {	import flash.display.MovieClip;	import flash.events.*;	import flash.text.TextField;	import flash.utils.Dictionary;	import flash.display.Shape;	import flash.display.Stage;	import edu.cmu.MyGlobal;		public class Key extends Floater {				// Constants:		// Public Properties:		// Private Properties:		private var principalTypes:Dictionary;				// Initialization:		public function Key(principalTypes:Dictionary) {			super();						this.principalTypes = principalTypes;						configUI();		}			// Public Methods:						// Protected Methods:		protected function configUI():void {						/*var titleLabel:TextField = MyGlobal.makeLabel("Key", 0x000000, 14, true);			titleLabel.x = 5;			titleLabel.y = 5;			this.addChild(titleLabel);*/						var x:int = 10;  //25			var y:int = 10;  //30			for (var type:String in principalTypes) {				var colorRect:Shape = new Shape;				colorRect.graphics.beginFill(MyGlobal.getColorForPrincipal(type), 1);				colorRect.graphics.drawRect(x, y, 15, 15);				colorRect.graphics.endFill();				this.addChild(colorRect);								x += colorRect.width + 5;								var label:TextField = MyGlobal.makeLabel(principalTypes[type], 0x000000);				label.x = x;				label.y = y;				this.addChild(label);								x += label.width + 15;			}						this.getChildAt(0).height = 35;  // 65			this.getChildAt(0).width = x + 10;  // + 20		}		protected override function setDefaultPosition():void {			var stage:Stage = Stage(this.parent);							this.x = stage.stageWidth - this.width - 10;			this.y = stage.stageHeight - this.height - 10;		}	}}