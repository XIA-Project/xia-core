package edu.cmu.layouts {	import flash.geom.Point;	import flash.display.Stage;	import edu.cmu.networkelements.NetworkElement;		public class RandomLayout extends Layout {				// Constants:		// Public Properties:		// Private Properties:			// Initialization:		public function RandomLayout() {		}			// Public Methods:		// Protected Methods:		protected override function GetPointForElement(element:NetworkElement):Point {			var x:int = Math.random() * element.stage.stageWidth;			var y:int = Math.random() * element.stage.stageHeight;			return new Point(x, y);		}	}	}