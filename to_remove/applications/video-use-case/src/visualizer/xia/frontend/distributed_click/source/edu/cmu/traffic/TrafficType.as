package edu.cmu.traffic {	public final class TrafficType {		public static const FINAL_INTENT_TYPE:String = "Final Intent Type";		public static const NEXT_HOP_TYPE:String = "Next Hop Type";				public static function GetRequestString(type:String) {			switch(type)			{				case FINAL_INTENT_TYPE:					return "/final";				case NEXT_HOP_TYPE:					return "/next";			}		}	}}