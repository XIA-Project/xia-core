srcdir =	@srcdir@
VPATH =		@srcdir@
top_srcdir =	@top_srcdir@

@BIND9_VERSION@

@BIND9_MAKE_INCLUDES@

CINCLUDES =	${DNS_INCLUDES} ${ISC_INCLUDES} -I ${top_srcdir}/../../XIASocket/API

CDEFINES =
CWARNINGS =

DNSLIBS =	../../../../lib/dns/libdns.@A@ @DNS_CRYPTO_LIBS@ ${top_srcdir}/../../XIASocket/API/libXsocket.a -lprotobuf -lpthread
ISCLIBS =	../../../../lib/isc/libisc.@A@ ${top_srcdir}/../../XIASocket/API/libXsocket.a -lprotobuf -lpthread

DNSDEPLIBS =	../../../../lib/dns/libdns.@A@ ${top_srcdir}/../../XIASocket/API/libXsocket.a -lprotobuf -lpthread
ISCDEPLIBS =	../../../../lib/isc/libisc.@A@ ${top_srcdir}/../../XIASocket/API/libXsocket.a -lprotobuf -lpthread

DEPLIBS =	${DNSDEPLIBS} ${ISCDEPLIBS}

LIBS =		${DNSLIBS} ${ISCLIBS} @LIBS@

TARGETS =	keycreate@EXEEXT@ keydelete@EXEEXT@

CREATEOBJS =	keycreate.@O@
DELETEOBJS =	keydelete.@O@

SRCS =		keycreate.c keydelete.c

@BIND9_MAKE_RULES@

all: keycreate@EXEEXT@ keydelete@EXEEXT@

keycreate@EXEEXT@: ${CREATEOBJS} ${DEPLIBS}
	${LIBTOOL_MODE_LINK} ${PURIFY} ${CC} ${CFLAGS} ${LDFLAGS} -o $@ ${CREATEOBJS} ${LIBS}

keydelete@EXEEXT@: ${DELETEOBJS} ${DEPLIBS}
	${LIBTOOL_MODE_LINK} ${PURIFY} ${CC} ${CFLAGS} ${LDFLAGS} -o $@ ${DELETEOBJS} ${LIBS}

clean distclean::
	rm -f ${TARGETS}

